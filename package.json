{
  "name": "minimalist",
  "version": "1.0.0",
  "description": "A starter framework for building web applications",
  "scripts": {
    "build": "node node_modules/webpack/bin/webpack --config ./webpack.production.config.js --json > build.json && NODE_ENV=production babel-node ./app.js",
    "start": "node node_modules/webpack/bin/webpack --config ./webpack.development.config.js --json > build.json && NODE_ENV=development babel-node ./app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rabyyuson/minimalist.git"
  },
  "author": "Raby Yuson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rabyyuson/minimalist/issues"
  },
  "homepage": "https://github.com/rabyyuson/minimalist#readme",
  "dependencies": {
    "debug": "^3.1.0",
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "express-react-views": "^0.10.5",
    "graphql": "^0.13.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-router-dom": "^4.3.1",
    "sass-loader": "^7.0.3",
    "webpack": "^4.12.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "node-sass": "^4.9.0",
    "postcss-loader": "^2.1.5",
    "react-hot-loader": "^4.3.3",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.2"
  }
}
